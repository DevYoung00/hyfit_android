<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:background="#f3f3f3">
    <ImageView
            android:id="@+id/process3"
            android:layout_width="268dp"
            android:layout_height="20dp"
            android:src="@drawable/process4_3"
            android:layout_alignParentEnd="true" android:layout_alignParentStart="true"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.497" app:layout_constraintVertical_bias="0.087"/>


    <TextView
            android:id="@+id/signup3"
            android:layout_below="@+id/process3"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="Sign up"
            android:fontFamily="sans-serif-medium"
            android:textStyle="normal"
            android:textSize="24sp"
            android:textColor="#000000"
            app:layout_constraintTop_toBottomOf="@+id/process3" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="52dp"
            app:layout_constraintHorizontal_bias="0.861"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/prev"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/process3"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.041"
            app:layout_constraintVertical_bias="0.528"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nickname"
            android:layout_below="@+id/process3"
            android:text="Nick name"
            android:textStyle="normal"
            android:textSize="14sp"
            android:textColor="#000000"
            android:fontFamily="sans-serif-medium"
            android:letterSpacing="0"
            app:layout_constraintTop_toBottomOf="@+id/process3" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="122dp"
            app:layout_constraintHorizontal_bias="0.105"/>

    <EditText
            android:id="@+id/textName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:fontFamily="sans-serif-light"
            android:ems="13" app:layout_constraintTop_toBottomOf="@+id/process3"
            android:hint=" at least 2 characters"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="184.5dp" app:layout_constraintHorizontal_bias="0.266" android:minHeight="48dp"
    />

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/birth"
            android:layout_below="@+id/process3"
            android:text="Gender"
            android:textStyle="normal"
            android:textSize="14sp"
            android:textColor="#000000"
            android:fontFamily="sans-serif-medium"
            android:letterSpacing="0"
            app:layout_constraintTop_toBottomOf="@+id/process3" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="300dp"
            app:layout_constraintHorizontal_bias="0.098"/>

    <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.51"
            app:layout_constraintVertical_bias="0.198">


    </RadioGroup>

    <ImageView
        android:id="@+id/joinNext"
        android:layout_width="310dp"
        android:layout_height="51dp"
        android:layout_marginTop="630dp"
        android:src="@drawable/rectangle_11"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/process3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/joinTextNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="642dp"
        android:fontFamily="sans-serif"
        android:gravity="end"
        android:letterSpacing="0"
        android:text="Next"
        android:textColor="#24629b"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/joinNext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/process3"
        app:layout_constraintVertical_bias="0.0" />

    <RadioButton
        android:id="@+id/woman"
        android:layout_width="138dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="woman"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.736"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/process3"
        app:layout_constraintVertical_bias="0.503" />

    <RadioButton
        android:id="@+id/man"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="man"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.263"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.559" />


</androidx.constraintlayout.widget.ConstraintLayout>